<?php 

namespace euro_hms\Api\Contracts;

interface ReceiptContract
{
	/*
     * Get All Receipt
     *
     * @return response
     */
    public function getReceiptList();

     /*
     * Delete Receipt
     *
     * @param  $id
     * @return response
     */

    public function deleteReceipt($id);

  	 /*
     * View Receipt
     *
     * @param  $id
     * @return response
     */

    public function viewReceipt($id,$type);



}